    {
        "id": "86021dc4f2272629",
        "type": "function",
        "z": "3cfec9319eee78cb",
        "name": "Set Color Parameters",
        "func": "var colorState = {\n    \"counter\": 0,\n    \"colors\": [\n        {\n            \"name\": \"red\",\n            \"color\": { \"rgb\": \"255,0,0\" }\n        },\n        {\n            \"name\": \"rose\",\n            \"color\": { \"rgb\": \"255,0,128\" }\n        },\n        {\n            \"name\": \"magents\",\n            \"color\": { \"rgb\": \"255,0,255\" }\n        },\n        {\n            \"name\": \"violet\",\n            \"color\": { \"rgb\": \"128,0,255\" }\n        },\n        {\n            \"name\": \"blue\",\n            \"color\": { \"rgb\": \"0,0,255\" }\n        },\n        {\n            \"name\": \"azure\",\n            \"color\": { \"rgb\": \"0,128,255\" }\n        },\n        {\n            \"name\": \"cyan\",\n            \"color\": { \"rgb\": \"0,255,255\" }\n        },\n        {\n            \"name\": \"spring green\",\n            \"color\": { \"rgb\": \"0,255,128\" }\n        },\n        {\n            \"name\": \"green\",\n            \"color\": { \"rgb\": \"0,255,0\" }\n        },\n        {\n            \"name\": \"chartreuse\",\n            \"color\": { \"rgb\": \"128,255,0\" }\n        },\n        {\n            \"name\": \"yellow\",\n            \"color\": { \"rgb\": \"255,255,0\" }\n        },\n        {\n            \"name\": \"orange\",\n            \"color\": { \"rgb\": \"255,128,0\" }\n        },\n        {\n            \"name\": \"white\",\n            \"color\": { \"rgb\": \"255,255,255\" }\n        }\n    ]\n}\nflow.set(\"colorState\", colorState);\n\nreturn colorState;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
